{
    "extension": [
        ".jsx"
    ],
    "include": [
        "**/*.js"
    ],
    "exclude": [
        "node_modules/**",
        "dist/**",
        "docs/**",
        "coverage/**",
        ".nyc_output/**",
        "test/**",
        "*.js"
    ],
    "reporter": [
        "text-summary",
        "cobertura",
        "lcov"
    ],
    "require": [
        "babel-register"
    ],
    "cache": true,
    "all": true,
    "check-coverage": false,
    "sourceMap": false,
    "instrument": false,
    "watermarks": {
        "lines": [
            70,
            90
        ],
        "functions": [
            70,
            90
        ],
        "branches": [
            70,
            90
        ],
        "statements": [
            70,
            90
        ]
    }
}
